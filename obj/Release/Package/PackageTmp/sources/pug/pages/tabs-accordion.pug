  - var title_h1 = "Tabs & Accordions"
  - var bread_2 = "Elements"
  - footer_class_adjuster = "bg-gray"
  - var section_full_adjuster = ""
  - var section_full_col_adjuster = ""
  - var brand_name_adjuster = "images/logo-2.png"

block title
  title Tabs & Accordions

block page
  // Page Header
  include ../elements/_header

  // Page Content
  main.page-content.text-md-start
    +breadcrumb_items(1)
    include ../section/tabs/_section_tabs-1
    include ../section/tabs/_section_tabs-2
    include ../section/tabs/_section_tabs-4
    include ../section/tabs/_section_tabs-5
    include ../section/tabs/_section_tabs-3
  // Page Footer
  include ../elements/_footer-widget




