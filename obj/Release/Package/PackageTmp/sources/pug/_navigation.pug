    .rd-navbar-top-panel
      .rd-navbar-top-panel-wrap
        dl.dl-horizontal-mod-1.login
          dt #[span.mdi.mdi-login.icon-xxs-mod-2]
          dd: a(href="login.html").text-sushi  Login/Registration
        .top-panel-inner
          dl.dl-horizontal-mod-1
            dt: span.material-icons-local_phone.icon-xxs-mod-2
            dd: a(href="callto:#")  1-800-1234-567
          dl.dl-horizontal-mod-1
            dt #[span.material-icons-drafts.icon-xxs-mod-2]
            dd:  a(href="mailto:#")  info@demolink.org
          address
            dl.dl-horizontal-mod-1
              dt: span.mdi.mdi-map-marker-radius.icon-xxs-mod-2
              dd: a(href="#").inset-11 795 Folsom Ave, Suite 600 San Francisco, CA 94107
        ul.list-inline
          li
            a.fa-facebook(href='#')
          li
            a.fa-twitter(href='#')
          li
            a.fa-pinterest-p(href='#')
          li
            a.fa-vimeo(href='#')
          li
            a.fa-google(href='#')
          li
            a.fa-rss(href='#')
        .btn-group
          a.btn.btn-sm.btn-primary(href="submit-property.html") Submit Property
